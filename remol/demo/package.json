{
  "name": "@remol/demo",
  "version": "2.1.1",
  "private": true,
  "description": "mol wire react bindings",
  "type": "module",
  "exports": {
    ".": "./-/index.js",
    "./*": "./-/*.js"
  },
  "types": "./-/index.d.ts",
  "main": "./-/index.js",
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "repository": "https://github.com/zerkalica/remol",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf - *.log",
    "build": "tsc --build && NODE_OPTIONS=--experimental-vm-modules jest && remol-boot bundle --publicUrl=/remol/",
    "dev": "tsc --build && NODE_OPTIONS=--experimental-vm-modules remol-boot dev"
  },
  "dependencies": {
    "@remol/core": "^2.1.1",
    "@remol/react": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typestyle": "^2.4.0"
  },
  "devDependencies": {
    "@remol/boot": "^2.1.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11"
  }
}
