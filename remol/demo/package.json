{
  "name": "@remol/demo",
  "version": "1.0.0",
  "private": true,
  "description": "mol wire react bindings",
  "exports": {
    ".": "./-/index.js",
    "./*": "./-/*.js"
  },
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "repository": "https://github.com/zerkalica/remol",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf - *.log",
    "bundle": "mkdir -p ./-/public && cp ./index.html ./-/public/ && esbuild --sourcemap --bundle ./browser.tsx --outfile=./-/public/browser.js",
    "build": "tsc --build && jest && npm run bundle",
    "dev": "npm run bundle -- --servedir=-/public"
  },
  "dependencies": {
    "@remol/core": "^1.0.0",
    "@remol/react": "^1.0.0",
    "autobind-decorator": "^2.4.0",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "typestyle": "^2.2.0"
  },
  "devDependencies": {
    "@remol/boot": "^1.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11"
  }
}
