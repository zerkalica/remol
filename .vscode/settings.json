{
  "typescript.tsdk": "node_modules/typescript/lib",
  "search.exclude": {
    "**/node_modules": true,
    "**/-*": true,
    "**/.swc": true
  },
  "files.exclude": {
    "**/.swc": true,
    "**/node_modules": true,
    "**/-*": false
