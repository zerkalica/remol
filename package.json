{
  "name": "remol-packages",
  "version": "1.0.0",
  "private": true,
  "description": "mol_wire react bindings",
  "repository": "https://github.com/zerkalica/remol",
  "author": "Stefan Zerkalica <zerkalica@gmail.com>",
  "license": "MIT",
